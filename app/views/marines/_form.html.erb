      <%= form_with(model: @marine) do |form| %>

        <div class="mb-3 mt-4">
          <%= form.text_field :first_name, {class: "form-control", placeholder: "Enter your Name here"}  %>
        </div>

        <div class="col-md-12 text-center mt-4 mb-2">
          <%= form.submit "Join Us!", class: "btn btn-primary btn-block" %>
        </div>
      <% end %>


