<div class="container">
  <h1 class="text-center m-5">Our Crew</h1>

  <div class="row mb-5">
    <% @marines.each do |marine| %>
      <div class="col-4 text-center">
        <%= marine.first_name %> <%= marine.last_name %>
      </div>
    <% end %>
  </div>

  <div class="row d-flex justify-content-center">
    <h2 class="text-center m-5">Get on board!</h2>
    <div class="col-md-6">
      <%= render "form" %>
    </div>
  </div>
</div>


